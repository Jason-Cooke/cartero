{
	"package-extends" : {
		"jqueryui-browser" : {
			"main" : "ui/jquery-ui.js",
			"style" : "themes/base/jquery-ui.css"
		}
	},

	"package-defaults" : {
		"style" : "*.scss,*.css",
		"template" : "*.tmpl",
		"image" : "*.png,*.jpg"
	},

	"global-transform" : {
		"script" : [ { "browserify-shim" : "./browserifyShimConfig.js" } ] // note here we pass in options to browserify-shim
	}
}