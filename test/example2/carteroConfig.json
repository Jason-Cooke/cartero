{
	"package-extends" : {
		"jqueryui-browser" : {
			"main" : "ui/jquery-ui.js",
			"style" : "themes/base/jquery-ui.css"
		}
	},

	"package-defaults" : {
		"style" : "*.css"
	},

	"global-transform" : {
		"script" : [ { "browserify-shim" : "./browserifyShimConfig.js" } ] // note here we pass in options to browserify-shim
	}
}

// what if browserify-shim relied on package stuff for a given package.
// but shit there might not even be a package 